<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="Warn" shutdownHook="disable">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%-5p] [%t] [%logger{36}]: %msg%n"/>
        </Console>

    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>